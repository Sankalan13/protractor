{
  "compilerOptions": {
    "target": "es6",
    "lib": [ "es2015" ],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "outDir": "./protractor/compiled-js-files",
    "types": [
      "node", "jasmine"
    ]
  },
  "exclude": [
    "node_modules",
    "plugins.ts"
  ]
}